# SPA fallback for React Router on OVH (Apache)
RewriteEngine On
RewriteBase /

# Laisse passer les fichiers/dossiers rÃ©els
RewriteRule ^index\.html$ - [L]
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule . - [L]

# Tout le reste -> index.html
RewriteRule ^.*$ /index.html [L]
